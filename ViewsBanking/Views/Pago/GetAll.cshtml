@model IEnumerable<ViewsBanking.Models.Pago>

@{
    ViewBag.Title = "GetAll";
}

<head>
    <title>Pago</title>
</head>
<h2>GetAll</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodigoServicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodigoCuenta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodigoMoneda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaHora)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Monto)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoServicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoCuenta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoMoneda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaHora)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.ActionLink("Borrar","Borrar","Pago",new {item.Codigo},null)
            @Html.ActionLink("Delete", "Delete", new { item.Codigo })
        </td>
    </tr>
}

</table>
